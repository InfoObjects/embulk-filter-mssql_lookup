exec:
  max_threads: 1
  min_output_tasks: 1
in:
  type: mysql
  host: localhost
  user: root
  password: 'passsword'
  database: test
  table: database_table
filters:
  - type: sqlserver
    host: Localhost
    driver_path: "..path../mssql-jdbc-10.2.0.jre17.jar"
    username: "user"
    password: "password"
    database: "test"
    table_name: car
    mapping_from:
    - car_name
    - company
    mapping_to:
    - model
    - brand
    new_columns:
     - { name: car_id, type: string }
     - { name: category, type: string }
     - { name: fuel_capacity, type: string }

out:
  type: mysql
  host: localhost
  user: root
  password: 'passsword'
  database: test
  table: output_table
  mode: truncate_insert
